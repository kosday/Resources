      LK.METHOD.LIST<-1>="LKSCHEMAS"
      LK.METHOD.LIST<-1>="LKPROPERTIES"
      LK.METHOD.LIST<-1>="GETTABLE"
      LK.METHOD.LIST<-1>="RESETCOMMONBLOCK"      
